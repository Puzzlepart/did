{
    "verbose": false,
    "ignore": [
        ".github",
        ".vscode",
        "bin",
        "client",
        "dist/public",
        "doc",
        "lib",
        "node_modules",
        "public",
        "resources",
        "scss",
        "server",
        "tasks",
        "typedoc-theme",
        ".deployment",
        ".gitignore",
        "app.js",
        "CHANGELOG.md",
        "CONTRIBUTING.md",
        "deploy.cmd",
        "nodemon.json",
        "README.md",
        "tsconfig.client.json",
        "tsconfig.server.json",
        "typedoc.json",
        "webpack.config.js"
    ],
    "delay": 2000,
    "execMap": {
        "rb": "ruby",
        "pde": "processing --sketch={{pwd}} --run"
    }
}