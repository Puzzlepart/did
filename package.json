{
  "name": "did365",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watch": "concurrently \"npm run watch:client:package\" \"npm run watch:server\" \"npm run watch:scss\"",
    "git:add:all": "git add --all",
    "start": "node ./bin/www",
    "watch:server": "nodemon --ignore client --ignore public --ignore lib ./bin/www",
    "build:client": "tsc --build",
    "build:scss": "node-sass --include-path scss scss/main.scss public/css/main.css",
    "watch:scss": "onchange -k -i -d 750 \"./**/*.scss\" -- npm run build:scss",
    "watch:client": "tsc --watch",
    "package:client": "npm run build:client && webpack --config webpack.config.js",
    "package:client:prod": "set NODE_ENV=production&&npm run build:client && webpack --config webpack.config.js",
    "watch:client:package": "onchange -k -i -d 750 \"./client/**/*.ts\" \"./client/**/*.tsx\" -- npm run package:client",
    "pregit:add:all": "npm run package:client:prod && npm run build:scss",
    "clean": "rimraf ./lib && rimraf ./public/js"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "2.0.0",
    "@types/react-autosuggest": "^9.3.13",
    "array-sort": "^1.0.0",
    "azure-storage": "2.10.3",
    "connect-azuretables": "1.0.20",
    "connect-flash": "0.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "^4.1.1",
    "dotenv": "8.2.0",
    "express": "4.16.4",
    "express-favicon": "^2.0.1",
    "express-graphql": "0.9.0",
    "express-pino-logger": "^4.0.0",
    "express-session": "1.17.0",
    "graphql": "14.5.8",
    "graphql-import": "0.7.1",
    "graphql-tools": "4.0.6",
    "hbs": "4.0.6",
    "helmet": "^3.21.2",
    "http-errors": "1.6.3",
    "morgan": "1.9.1",
    "node-fetch": "2.6.0",
    "passport": "0.4.0",
    "passport-azure-ad": "4.2.0",
    "pino": "^5.14.0",
    "simple-oauth2": "2.4.0",
    "string-format": "^2.0.0",
    "string-similarity": "^3.0.0",
    "string-strip-html": "4.3.8",
    "supports-color": "^7.1.0",
    "underscore": "^1.9.1",
    "uuid": "^3.3.3",
    "validator": "^12.2.0"
  },
  "devDependencies": {
