  "scripts": {
    "watch": "concurrently \"webpack\" \"npm run watch:server\" \"node tasks/open.js\" \"node tasks/localtunnel.js\"",
    "start": "ts-node ./server",
    "watch:server": "ts-node-dev --ignore-watch node_modules server",
    "package:client": "webpack",
    "lint": "npm run lint:client && npm run prettier:client && npm run lint:server && npm run prettier:server",
    "lint:client": "eslint --ext .ts,.tsx ./client --color --fix",
    "lint:server": "eslint --ext .js ./server --color --fix",
    "prettier:client": "prettier --config .prettierrc 'client/**/*.ts' --write",
    "prettier:server": "prettier --config .prettierrc 'server/**/*.js' --write",
    "makeDoc": "rimraf ./doc && typedoc --options typedoc.json && graphdoc",
    "tests": "mocha tests --exit",
    "clean": "rimraf /public/js"
  },
