{
  "name": "did365",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./server.js",
    "start:dev": "set NODE_ENV=development&&node ./server.js",
    "watch:server": "set NODE_ENV=development&&nodemon ./server.js",
    "build:client": "tsc --build",
    "watch:client": "tsc --watch",
    "webpack:client": "webpack --config webpack.config.js",
    "package:client": "npm run build:client && npm run webpack:client",
    "watch:client:development": "set NODE_ENV=development&&npm-watch package:client",
    "watch:client:production": "set NODE_ENV=production&&npm-watch package:client"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ],
    "delay": "1000"
  },
  "watch": {
    "package": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx",
      "quiet": false,
      "legacyWatch": true,
      "delay": 500,
      "runOnChangeOnly": false
    }
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "2.0.0",
    "azure-storage": "2.10.3",
    "connect-azuretables": "^1.0.20",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "2.6.9",
    "dotenv": "8.2.0",
    "express": "4.16.4",
    "express-graphql": "^0.9.0",
    "express-session": "1.17.0",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.6",
    "hbs": "4.0.6",
    "http-errors": "1.6.3",
    "morgan": "1.9.1",
    "node-fetch": "^2.6.0",
    "passport": "0.4.0",
    "passport-azure-ad": "4.2.0",
    "simple-oauth2": "3.1.0",
    "string-strip-html": "^4.3.8",
    "uuid": "^3.3.3",
    "@apollo/react-hooks": "^3.1.3",
    "@pnp/common": "^1.3.7",
    "@uifabric/icons": "^7.3.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "apollo-utilities": "^1.3.2",
    "axios": "^0.19.0",
    "axios-cache-adapter": "^2.5.0",
    "chalk": "^3.0.0",
    "graphql-tag": "^2.10.1",
    "highcharts": "^7.2.1",
    "highcharts-react-official": "^2.2.2",
    "loglevel": "^1.6.6",
    "loglevel-plugin-prefix": "^0.8.4",
    "moment": "^2.24.0",
    "office-ui-fabric-react": "7.64.2",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "scriptjs": "^2.5.9",
    "weeknumber": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@types/es6-promise": "^3.3.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/scriptjs": "^0.0.2",
    "@types/webpack-env": "^1.14.1",
    "babel-loader": "^8.0.6",
    "npm-watch": "^0.6.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "nodemon": "^2.0.1"
  }
}