{
  "name": "did365",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "watch": "concurrently \"npm run watch:client:package\" \"npm run watch:server\"",
    "start:dev": "set NODE_ENV=development&&node ./bin/www",
    "watch:server": "set NODE_ENV=development&&nodemon --ignore client --ignore public --ignore lib ./bin/www",
    "build:client": "tsc --build",
    "watch:client": "tsc --watch",
    "package:client": "npm run build:client && webpack --config webpack.config.js",
    "package:client:prod": "set NODE_ENV=production&&npm run build:client && webpack --config webpack.config.js",
    "watch:client:package": "onchange -k -i -d 1500 \"./client/**/*.ts\" \"./client/**/*.tsx\" -- npm run package:client"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "2.0.0",
    "azure-storage": "2.10.3",
    "connect-azuretables": "1.0.20",
    "connect-flash": "0.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "2.6.9",
    "dotenv": "8.2.0",
    "express": "4.16.4",
    "express-graphql": "0.9.0",
    "express-session": "1.17.0",
    "graphql": "14.5.8",
    "graphql-import": "0.7.1",
    "graphql-tools": "4.0.6",
    "hbs": "4.0.6",
    "helmet": "^3.21.2",
    "http-errors": "1.6.3",
    "morgan": "1.9.1",
    "node-fetch": "2.6.0",
    "passport": "0.4.0",
    "passport-azure-ad": "4.2.0",
    "simple-oauth2": "3.1.0",
    "string-strip-html": "4.3.8"
  },
  "devDependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@babel/core": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@pnp/common": "1.3.7",
    "@types/get-value": "^3.0.1",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/scriptjs": "0.0.2",
    "@types/webpack-env": "1.14.1",
    "@uifabric/icons": "7.3.0",
    "apollo-cache": "1.3.2",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "1.5.16",
    "apollo-utilities": "1.3.2",
    "axios": "0.18.1",
    "babel-loader": "8.0.6",
    "chalk": "3.0.0",
    "concurrently": "5.0.0",
    "get-value": "^3.0.1",
    "graphql-tag": "2.10.1",
    "highcharts": "7.2.1",
    "highcharts-react-official": "2.2.2",
    "loglevel": "1.6.6",
    "loglevel-plugin-prefix": "0.8.4",
    "moment": "2.24.0",
    "nodemon": "1.19.4",
    "office-ui-fabric-react": "7.64.2",
    "onchange": "^6.1.0",
    "react": "16.12.0",
    "react-apollo": "3.1.3",
    "react-dom": "16.12.0",
    "scriptjs": "2.5.9",
    "typescript": "^3.7.2",
