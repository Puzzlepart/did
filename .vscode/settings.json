{
  "files.exclude": {
    "node_modules": true,
    "client/**/*.js": true,
    "client/**/*.js.map": true,
    "client/**/*.d.ts": true,
    "server/**/*.js": true,
    "server/**/*.js.map": true,
    "server/views/index.hbs": true,
    "server/public/js": true,
    "dist": true,
    ".assets": true,
    "server/docs": true,
    "client/docs": true
  },
  "typescript.autoClosingTags": false,
  "javascript.validate.enable": false,
  "i18n-ally.localesPaths": "./client/i18n",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.sourceLanguage": "en-GB",
  "i18n-ally.displayLanguage": "en-GB",
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "editor.folding": true
}
