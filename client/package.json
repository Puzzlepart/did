    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "apollo-utilities": "^1.3.2",
    "axios": "^0.19.0",
    "axios-cache-adapter": "^2.5.0",
    "chalk": "^3.0.0",
    "graphql-tag": "^2.10.1",
    "highcharts": "^7.2.1",
    "highcharts-react-official": "^2.2.2",
    "loglevel": "^1.6.6",
    "loglevel-plugin-prefix": "^0.8.4",
    "moment": "^2.24.0",
    "office-ui-fabric-react": "7.64.2",
