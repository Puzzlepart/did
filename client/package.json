{
  "name": "did365appreact",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc --watch",
    "webpack": "webpack --config webpack.config.js",
    "package": "npm run build && npm run webpack",
    "watch:package:development": "set NODE_ENV=development&&npm-watch package",
    "watch:package:production": "set NODE_ENV=production&&npm-watch package"
  },
  "watch": {
    "package": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx",
      "quiet": false,
      "legacyWatch": true,
      "delay": 500,
      "runOnChangeOnly": false
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
