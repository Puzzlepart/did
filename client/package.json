    "@types/react-dom": "^16.9.4",
    "@types/scriptjs": "^0.0.2",
    "@types/webpack-env": "^1.14.1",
    "babel-loader": "^8.0.6",
    "npm-watch": "^0.6.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
