{
  "name": "did-server",
  "includeVersion": true,
  "out": "server/.docs",
  "readme": "none",
  "excludeInternal": true,
  "excludeExternals": true,
  "externalPattern": [
    "node_modules/**/*.{ts,tsx,js}"
  ],
  "hideGenerator": true,
  "gitRevision": "dev",
  "categorizeByGroup": true,
  "categoryOrder": ["Resolver", "*"],
  "entryPoints": [
    "server/index.ts",
    "server/app.ts",
    "server/graphql/index.ts",
    "server/middleware/index.ts",
    "server/routes/auth.ts",
    "server/services/index.ts",
    "server/utils/index.ts"
  ]
}
